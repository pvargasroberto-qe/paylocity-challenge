            {
              "name": "POST - Create Employee",
              "event": [
                {
                  "listen": "prerequest",
                  "script": {
                    "exec": [
                      "const randomInt = Math.floor(Math.random() * 32);\r",
                      "pm.environment.set(\"dependants\", randomInt)"
                    ],
                    "type": "text/javascript",
                    "packages": {},
                    "requests": {}
                  }
                },
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "pm.test(\"Status is 200 or 201\", function () {\r",
                      "  pm.expect(pm.response.code).to.be.oneOf([200, 201]);\r",
                      "});\r",
                      "\r",
                      "const json = pm.response.json();\r",
                      "const dependants = pm.response.json().dependants;\r",
                      "\r",
                      "pm.test(\"Response has id\", function () {\r",
                      "  pm.expect(json).to.have.property(\"id\");\r",
                      "  pm.expect(json.id).to.be.a(\"string\").and.not.empty;\r",
                      "});\r",
                      "\r",
                      "pm.environment.set(\"employeeId\", json.id);\r",
                      "pm.environment.set(\"firstName\", json.firstName);\r",
                      "pm.environment.set(\"lastName\", json.lastName);\r",
                      "\r",
                      "pm.test(\"Gross should be 2000 when salary=52000\", function () {\r",
                      "  if (json.gross !== undefined) {\r",
                      "    pm.expect(Number(json.gross)).to.be.closeTo(2000, 0.01);\r",
                      "  }\r",
                      "});\r",
                      "\r",
                      "pm.test(\"benefitsCost follows formula\", function () {\r",
                      "  if (json.benefitsCost !== undefined) {\r",
                      "    const expected = (1000 + 500 * Number(dependants)) / 26;\r",
                      "    pm.expect(Number(json.benefitsCost)).to.be.closeTo(expected, 0.01);\r",
                      "  }\r",
                      "});\r",
                      "\r",
                      "pm.test(\"net = gross - benefitsCost\", function () {\r",
                      "  if (json.net !== undefined && json.gross !== undefined && json.benefitsCost !== undefined) {\r",
                      "    const expected = Number(json.gross) - Number(json.benefitsCost);\r",
                      "    pm.expect(Number(json.net)).to.be.closeTo(expected, 0.02);\r",
                      "  }\r",
                      "});"
                    ],
                    "type": "text/javascript",
                    "packages": {},
                    "requests": {}
                  }
                }
